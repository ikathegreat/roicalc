<Window x:Class="SNROI.Views.ReportDesignerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SNROI"
        xmlns:dxrud="http://schemas.devexpress.com/winfx/2008/xaml/reports/userdesigner"
        mc:Ignorable="d"
        Title="Report Editor" Height="650" Width="800" Loaded="Window_Loaded">
    <Grid>
        <dxrud:ReportDesigner x:Name="reportDesigner" >
            <dxrud:ReportDesignerBrowserView/>
        </dxrud:ReportDesigner>
    </Grid>
</Window>
